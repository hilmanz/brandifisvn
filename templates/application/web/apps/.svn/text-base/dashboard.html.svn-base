<!-- ki -->
<div class="page_section" id="project-page">
    <div id="container">
        <div class="titlebox">
            <h2 class="fl"><span class="icon-books">&nbsp;</span> SUBSCRIBES</h2>
            <h2 class="fr"><a href="{$basedomain}home" class="button2">Home</a></h2>
        </div><!-- end .titlebox -->
        <div class="content">
            <div class="summary">
                <p>Touchbase helps you to Subscribes Any Kind of problem That u Feel to Ask</p>
            </div><!-- end .summary -->

            <form id="forms" class="forms" method="post" action="" enctype="multipart/form-data">
            <div id="new-project" class="boxcontent">
                <section class="step1">
                    <h3>SUBSCRIBES</h3>
                    <p>Fill in the details below to submit your Subscribes</p>
                    <div class="row">
                        <label for="projectName">NAME<br></label>
                        <input id="projectName" name="name" type="text" class="required" value={$status.name}><br><label class="msg_name" style="color: red;">{$status.msgn}	</label>
                    </div><!-- end .row -->
                    <div class="row">
                        <label for="uname">EMAIL </label>
                        <input id="uname" name="email" type="text" class="required" value={$status.email}>
						  <br><label class="msg_name" style="color: red;">{$status.email_ngga_valid}</label>
                    </div><!-- end .row -->
                    <div class="row">
                        <label for="userName">CITY <br></label>
					
                        <input id="userName" name="alamat" type="text" class="required" value={$status.alamat}>
                   <br><label class="msg_name" style="color: red;">{$status.alamat_ngga_valid}</label>
				 </div><!-- end .row -->
                <div class="row row-submit">
                    <input type="hidden" name="submit" value="1">
                        <input type="submit" value="SAVE" class="button3"  />
                        
                    </div><!-- end .row -->
                        
                    </div><!-- end .row -->
                </section>
            </div><!-- end #wizard -->
            </form>
        </div><!-- end .content -->
    </div><!-- end #container -->
</div><!-- end #home -->